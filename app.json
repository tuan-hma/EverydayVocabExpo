{
  "name": "MoodyDiary",
  "displayName": "MoodyDiary",
  "build": {
    "production": {
      "node": "14.19.3"
    }
  },
  "expo": {
    "name": "MoodyDiary",
    "slug": "MoodyDiary",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.tuanhma.MoodyDiary"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
